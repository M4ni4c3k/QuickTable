{
  "name": "quicktable",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "services:install": "npm install --prefix services/order-service && npm install --prefix services/reservation-service && npm install --prefix services/table-service && npm install --prefix services/menu-service && npm install --prefix services/hours-service && npm install --prefix services/notification-service && npm install --prefix services/realtime-service && npm install --prefix services/auth-service && npm install --prefix services/api-gateway",
    "services:start": "npm run services:start:gateway & npm run services:start:order & npm run services:start:reservation & npm run services:start:table & npm run services:start:menu & npm run services:start:hours & npm run services:start:notification & npm run services:start:realtime & npm run services:start:auth",
    "services:start:gateway": "cd services/api-gateway && npm start",
    "services:start:order": "cd services/order-service && npm start",
    "services:start:reservation": "cd services/reservation-service && npm start",
    "services:start:table": "cd services/table-service && npm start",
    "services:start:menu": "cd services/menu-service && npm start",
    "services:start:hours": "cd services/hours-service && npm start",
    "services:start:notification": "cd services/notification-service && npm start",
    "services:start:realtime": "cd services/realtime-service && npm start",
    "services:start:auth": "cd services/auth-service && npm start",
    "services:dev": "npm run services:dev:gateway & npm run services:dev:order & npm run services:dev:reservation & npm run services:dev:table & npm run services:dev:menu & npm run services:dev:hours & npm run services:dev:notification & npm run services:dev:realtime & npm run services:dev:auth",
    "services:dev:gateway": "cd services/api-gateway && npm run dev",
    "services:dev:order": "cd services/order-service && npm run dev",
    "services:dev:reservation": "cd services/reservation-service && npm run dev",
    "services:dev:table": "cd services/table-service && npm run dev",
    "services:dev:menu": "cd services/menu-service && npm run dev",
    "services:dev:hours": "cd services/hours-service && npm run dev",
    "services:dev:notification": "cd services/notification-service && npm run dev",
    "services:dev:realtime": "cd services/realtime-service && npm run dev",
    "services:dev:auth": "cd services/auth-service && npm run dev",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "start": "start-all.bat",
    "start:all": "start-all.bat",
    "start:services": "start-all.bat",
    "create:test-accounts": "node src/scripts/create-test-accounts.js"
  },
  "dependencies": {
    "@react-firebase/auth": "^0.2.10",
    "@react-firebase/firestore": "^0.5.5",
    "firebase": "^11.7.1",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^12.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.4.35",
    "prettier": "3.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
